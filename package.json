{
    "name": "vscode-gmusic",
    "displayName": "vscode-gmusic",
    "description": "Connecting VS Code with the Unofficial Google Play Music Desktop Player",
    "version": "1.3.0",
    "publisher": "nbdeg",
    "author": {
        "name": "Nick DeGroot"
    },
    "license": "MIT",
    "homepage": "https://github.com/nbd9/vscode-gmusic",
    "bugs": {
        "url": "https://github.com/nbd9/vscode-gmusic/issues",
        "email": "contact@nbdeg.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/nbd9/vscode-gmusic.git"
    },
    "icon": ".github/icon.png",
    "engines": {
        "vscode": "^1.18.0"
    },
    "keywords": [
        "Google Play Music",
        "Google Play Music Desktop Player",
        "GPMDP",
        "Play Music"
    ],
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "files": [
        "/out"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "gmusic.playpause",
                "title": "Toggle Play Pause",
                "icon": "src/assets/ppause.png",
                "category": "Google Play Music"
            },
            {
                "command": "gmusic.shuffle",
                "title": "Toggle Shuffle",
                "icon": "src/assets/shuffle.png",
                "category": "Google Play Music"
            },
            {
                "command": "gmusic.skip",
                "title": "Skip Song",
                "icon": "src/assets/forward.png",
                "category": "Google Play Music"
            },
            {
                "command": "gmusic.rewind",
                "title": "Rewind Song",
                "icon": "src/assets/back.png",
                "category": "Google Play Music"
            },
            {
                "command": "gmusic.setThumbs",
                "title": "Like/Dislike Song",
                "category": "Google Play Music"
            },
            {
                "command": "gmusic.restart",
                "title": "Restart",
                "category": "Google Play Music"
            },
            {
                "command": "gmusic.cycleRepeat",
                "title": "Cycle Repeat Mode",
                "category": "Google Play Music"
            }
        ],
        "menus": {
            "touchBar": [
                {
                    "command": "gmusic.rewind",
                    "group": "GMusic@1"
                },
                {
                    "command": "gmusic.playpause",
                    "group": "GMusic@2"
                },
                {
                    "command": "gmusic.skip",
                    "group": "GMusic@3"
                },
                {
                    "command": "gmusic.shuffle",
                    "group": "GMusic@4"
                }
            ]
        }
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.ts": [
            "eslint --fix",
            "git add"
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "@types/node": "^12.6.1",
        "@types/ws": "^6.0.1",
        "@typescript-eslint/eslint-plugin": "^1.11.0",
        "@typescript-eslint/parser": "^1.11.0",
        "eslint": "^6.0.1",
        "eslint-config-prettier": "^6.0.0",
        "eslint-config-standard": "^12.0.0",
        "eslint-plugin-import": "^2.18.0",
        "eslint-plugin-node": "^9.1.0",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.0.0",
        "husky": "^3.0.0",
        "lint-staged": "^9.1.0",
        "prettier": "^1.18.2",
        "typescript": "^3.5.3",
        "vscode": "^1.1.35"
    },
    "dependencies": {
        "vscode-cache": "^0.3.0",
        "ws": "^7.1.0"
    }
}
